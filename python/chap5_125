import pandas as pd

dict_data = {'동물':['Dog','Cat','Tiger','Lion','Monkey'],'나이':[7,9,2,3,1]}
df1 = pd.DataFrame(dict_data)

print(type(df1))

print(df1)

df2=pd.DataFrame([['Dog','7'],['Cat','9'],['Tiger','2'],['Lion','3'],['Monkey','1']],
index = ['0','1','2','3','4'], columns = ['동물','나이'])

print(df2)
